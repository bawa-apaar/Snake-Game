class SnakeGame extends JFrame
{

	SnakeGame()
	{
		add(new MyPanel());
		setSize(600,600);
		//setUndecorated(true);
		setLocationRelativeTo(null);
		setVisible(true);
		
		
	}
	public static void main(String gg[])
	{
		new SnakeGame();
	}
}
